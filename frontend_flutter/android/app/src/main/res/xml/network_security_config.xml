<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!-- Allow HTTPS connections to production server -->
    <domain-config cleartextTrafficPermitted="false">
        <domain includeSubdomains="true">zubidauction.duckdns.org</domain>
        <trust-anchors>
            <certificates src="system" />
        </trust-anchors>
    </domain-config>
</network-security-config>

